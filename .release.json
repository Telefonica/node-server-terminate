{
  "non-interactive": false,
  "src": {
    "beforeStartCommand": "npm run lint && npm run test",
    "beforeStageCommand": "npm run changelog"
  },
  "commitMessage": "[[CHORE]] Bump version %s",
  "tagName": "v%s",
  "tagAnnotation": "v%s",
  "github": {
    "release": true,
    "releaseName": "v%s",
    "tokenRef": "RELEASEIT_GITHUB_TOKEN"
  },
  "changelogCommand": "npm run -s changelog-gh-release",
  "npm": {
    "publish": true
  }
}
